      "source": [
        "import numpy as np\n",
        "\n",
        "def compute_cosine(v1, v2):\n",
        "    cos_sim = np.dot(v1, v2) / (np.linalg.norm(v1) * np.linalg.norm(v2))\n",
        "    return cos_sim\n",
        "\n",
        "v1 = np.array([1, 2, 3, 4])\n",
        "v2 = np.array([1, 0, 3, 0])\n",
        "print(compute_cosine(v1, v2))\n",
        "# C"
      ]
    }
